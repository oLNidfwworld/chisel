// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model RealtyObjectSection {
  id Int @id 
}

model RealtyObject {
  id   Int     @id 
  name String? 
}

model RealtyObjectProp {
  id Int @id
  type String
  name String
  code String
  value String
}